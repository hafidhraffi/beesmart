{
  "rewrites": [
    {
      "source": "/blog",
      "destination": "https://beesmart-sm.vercel.app/blog"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://beesmart-sm.vercel.app/blog/:path*"
    },
    {
      "source": "/sitemap.xml",
      "destination": "https://beesmart-sm.vercel.app/sitemap.xml"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}